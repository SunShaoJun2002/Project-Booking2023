<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="SE.mapper.FeatureMapper">
    <select id="selectById" resultType="SE.pojo.Feature">
        select * from feature where id=#{hotelId}
    </select>
    <select id="selectAll" resultType="SE.pojo.Feature">
        select * from feature
    </select>

</mapper>