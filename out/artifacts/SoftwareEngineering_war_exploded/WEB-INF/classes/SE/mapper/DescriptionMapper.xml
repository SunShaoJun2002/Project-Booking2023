<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="SE.mapper.DescriptionMapper">
    <select id="TestDBlink" resultType="SE.pojo.Description">
        select * from description where id=1
    </select>
    <select id="searchByID" resultType="SE.pojo.Description">
        select * from description where id=#{id}
    </select>
</mapper>